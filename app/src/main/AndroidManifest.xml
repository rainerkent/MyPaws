<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.google.tbuilding">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/paws"
        android:label="@string/app_name"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".Loading">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SignInActivity" />
        <activity android:name=".HomeActivity" />
        <activity android:name=".NewClaimActivity" />
        <activity android:name=".SubmitClaimActivity" />
        <activity android:name=".ScoopActivity" />
        <activity android:name=".VerifyClaimActivity" />
        <activity android:name=".RegisterActivity" />
        <activity android:name=".SignUpActivity" />
    </application>

</manifest>
